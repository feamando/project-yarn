{
  "name": "project-yarn",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "vitest",
    "test:accessibility": "vitest --run src/tests/accessibility",
    "test:accessibility:watch": "vitest src/tests/accessibility",
    "test:accessibility:coverage": "vitest --run --coverage src/tests/accessibility",
    "test:accessibility:performance": "vitest --run --reporter=json --outputFile=accessibility-performance-results.json src/tests/accessibility",
    "test:accessibility:regression": "node scripts/accessibility-regression-test.js",
    "accessibility:audit": "axe --tags wcag2a,wcag2aa,wcag21aa http://localhost:4173",
    "accessibility:lighthouse": "lighthouse http://localhost:4173 --only-categories=accessibility --output=json --output-path=lighthouse-accessibility.json",
    "accessibility:validate": "node scripts/validate-accessibility-compliance.js",
    "accessibility:performance": "node scripts/check-accessibility-performance.js",
    "accessibility:report": "node scripts/generate-accessibility-report.js",
    "setup-models": "node scripts/setup-models.js",
    "verify-models": "node scripts/verify-models.js",
    "update-models": "node scripts/update-models.js",
    "models:install": "node scripts/setup-models.js --install-all",
    "models:check": "node scripts/verify-models.js --quiet",
    "models:status": "node scripts/setup-models.js --status",
    "models:clean": "node scripts/update-models.js --cleanup",
    "validate-gitignore": "node scripts/validate-gitignore.js",
    "check-gitignore": "node scripts/validate-gitignore.js --quiet",
    "install-hooks": "node scripts/pre-commit-hook.js --install",
    "setup-git-hooks": "node scripts/setup-git-hooks.js",
    "pre-commit": "node scripts/pre-commit-hook.js",
    "git:validate": "npm run validate-gitignore && npm run models:check",
    "ci:setup": "node scripts/ci-setup.js",
    "ci:validate": "node scripts/ci-setup.js --check",
    "ci:test": "npm run lint && npm run test && npm run git:validate",
    "verify-setup": "node scripts/ci-setup.js --check && npm run verify-models --quiet",
    "models:version-check": "node scripts/model-versioning.js check",
    "models:version-update": "node scripts/model-versioning.js update",
    "release:generate-key": "bash scripts/generate-signing-key.sh",
    "release:build": "npm run build && npm run tauri build",
    "release:tag": "node scripts/create-release-tag.js",
    "release:check": "node scripts/check-release-readiness.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:components": "node scripts/lint-components.js",
    "lint:components:fix": "node scripts/lint-components.js --fix",
    "lint:components:strict": "node scripts/lint-components.js --strict",
    "lint:ui": "node scripts/lint-components.js --type=ui",
    "lint:ui:fix": "node scripts/lint-components.js --type=ui --fix",
    "lint:common": "node scripts/lint-components.js --type=common",
    "lint:features": "node scripts/lint-components.js --type=features",
    "lint:performance": "node scripts/lint-components.js --type=performance",
    "lint:hooks": "node scripts/lint-components.js --type=hooks",
    "lint:hooks:fix": "node scripts/lint-components.js --type=hooks --fix",
    "generate:component": "node scripts/create-component.js",
    "generate:components": "node scripts/generate-components-batch.js",
    "generate:sample": "node scripts/generate-components-batch.js --sample",
    "generate:validate": "node scripts/generate-components-batch.js --validate",
    "e2e:setup": "wdio install",
    "e2e:test": "wdio run wdio.conf.js",
    "e2e:test:headless": "wdio run wdio.conf.js --headless",
    "e2e:test:debug": "wdio run wdio.conf.js --debug",
    "e2e:visual": "wdio run wdio.conf.js --suite visual",
    "e2e:visual:baseline": "wdio run wdio.conf.js --suite baseline",
    "e2e:visual:update": "wdio run wdio.conf.js --suite baseline --updateBaseline",
    "e2e:visual:ci": "wdio run wdio.conf.js --suite visual --headless",
    "models:version-rollback": "node scripts/model-versioning.js rollback",
    "models:version-history": "node scripts/model-versioning.js history",
    "models:version-list": "node scripts/model-versioning.js list",
    "models:version-cleanup": "node scripts/model-versioning.js cleanup"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.15",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@tailwindcss/postcss": "^4.1.11",
    "@tauri-apps/api": "^2.7.0",
    "ai": "^5.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "focus-trap": "^7.6.5",
    "focus-trap-react": "^11.0.4",
    "lucide-react": "^0.525.0",
    "mermaid": "^11.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-panels": "^3.0.3",
    "react-window": "^1.8.8",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@axe-core/react": "^4.10.2",
    "@tauri-apps/cli": "^2.7.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@wdio/cli": "^8.24.0",
    "@wdio/local-runner": "^8.24.0",
    "@wdio/mocha-framework": "^8.24.0",
    "@wdio/spec-reporter": "^8.24.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "eslint": "^8.45.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "mocha": "^10.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^3.2.4",
    "webdriverio": "^8.24.0"
  }
}
